@Article{pang2011,
  Title                    = {Accelerating simultaneous algebraic reconstruction technique with motion compensation using CUDA-enabled GPU},
  Author                   = {Pang, Wai-Man and Qin, Jing and Lu, Yuqiang and Xie, Yongming and Chui, Chee-Kong and Heng, Pheng-Ann},
