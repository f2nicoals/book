@Article{buhlmannlaser,
  Title                    = {Laser speckle based background oriented schlieren measurements in a fire backlayering front},
  Author                   = {B√ºhlmann, Philipp and Meier, Alexander H and Ehrensperger, Martin and R√∂sgen, T.sgen, Thomas},
  Journal                  = {17th International Symposium on Applications of Laser Techniques to Fluid Mechanics},
  Year                     = {2014},

  Abstract                 = {The recently introduced laser speckle based background oriented schlieren technique (speckle BOS) is used to
study a variable density flow simulating the spread of hot gases in a tunnel fire. The specific advantage of the speckle
BOS method is the possibility to measure in flow fields close to the schlieren reference pattern while maintaining a high
